{
  "name": "browserosaurus",
  "version": "10.12.1",
  "description": "The browser prompter for macOS",
  "keywords": [
    "macOS",
    "Electron",
    "Browser chooser"
  ],
  "homepage": "https://browserosaurus.com/",
  "bugs": {
    "url": "https://github.com/will-stone/browserosaurus/issues"
  },
  "repository": "https://github.com/will-stone/browserosaurus",
  "license": "MIT",
  "author": "Will Stone",
  "main": ".webpack/main",
  "scripts": {
    "docs": "npx http-server ./docs",
    "icns": "cd ./src/images/icon && ./scripts/png2icns.sh icon.png",
    "lint": "eslint . --ignore-path .gitignore",
    "list-apps": "ts-node ./scripts/listInstalledApps",
    "make": "electron-forge make --skip-package",
    "notarize": "ts-node ./scripts/notarize.ts",
    "prepackage": "rimraf ./out/",
    "package": "NODE_ENV=production electron-forge package --platform=darwin --arch=x64",
    "release": "yarn version && yarn package && yarn notarize && yarn make",
    "start": "ENV=DEV ELECTRON_DISABLE_SECURITY_WARNINGS=true electron-forge start",
    "test": "jest",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "appBundleId": "com.browserosaurus",
        "asar": true,
        "appCategoryType": "public.app-category.developer-tools",
        "packageManager": "yarn",
        "extendInfo": "config/Info.plist",
        "osxSign": {
          "gatekeeper-assess": false,
          "hardened-runtime": true,
          "entitlements": "config/entitlements.mac.plist",
          "entitlements-inherit": "config/entitlements.mac.plist"
        },
        "icon": "src/main/static/icon/icon.icns",
        "protocols": [
          {
            "name": "HTTP link",
            "schemes": [
              "http",
              "https"
            ]
          },
          {
            "name": "File",
            "schemes": [
              "file"
            ]
          }
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "prettier": "@will-stone/prettier-config",
  "eslintConfig": {
    "extends": [
      "@will-stone/eslint-config",
      "@will-stone/eslint-config/node",
      "@will-stone/eslint-config/react",
      "@will-stone/eslint-config/jest",
      "@will-stone/eslint-config/typescript"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/docs/",
      "<rootDir>/out/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/docs/",
      "<rootDir>/out/"
    ]
  },
  "dependencies": {
    "@browser-logos/brave": "^3.0.10",
    "@browser-logos/brave-beta": "^1.0.8",
    "@browser-logos/brave-dev": "^1.0.8",
    "@browser-logos/brave-nightly": "^1.0.5",
    "@browser-logos/chrome": "^1.0.15",
    "@browser-logos/chrome-canary": "^1.1.7",
    "@browser-logos/chromium": "^1.0.6",
    "@browser-logos/edge": "^2.0.3",
    "@browser-logos/edge-beta": "^2.0.2",
    "@browser-logos/edge-canary": "^2.0.1",
    "@browser-logos/edge-dev": "^2.0.2",
    "@browser-logos/firefox": "^3.0.4",
    "@browser-logos/firefox-developer-edition": "^3.0.2",
    "@browser-logos/firefox-nightly": "^3.0.2",
    "@browser-logos/iridium": "^2.0.5",
    "@browser-logos/maxthon": "^1.0.5",
    "@browser-logos/opera": "^1.1.8",
    "@browser-logos/opera-beta": "^1.0.7",
    "@browser-logos/opera-developer": "^1.0.8",
    "@browser-logos/opera-gx": "^1.0.6",
    "@browser-logos/qutebrowser": "^1.0.2",
    "@browser-logos/safari": "^2.0.0",
    "@browser-logos/safari-technology-preview": "^2.0.1",
    "@browser-logos/tor": "^2.0.2",
    "@browser-logos/vivaldi": "^2.1.7",
    "@browser-logos/vivaldi-snapshot": "^1.0.6",
    "@browser-logos/yandex": "^1.0.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@reduxjs/toolkit": "^1.4.0",
    "animate.css": "^4.1.0",
    "app-exists": "^1.1.0",
    "clsx": "^1.1.1",
    "electron-is-dev": "^1.1.0",
    "electron-notarize": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.1.1",
    "p-filter": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "semver": "^7.3.2",
    "tailwindcss": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-dmg": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@types/pino": "^6.3.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/semver": "^7.3.1",
    "@will-stone/eslint-config": "^1.17.1",
    "@will-stone/prettier-config": "^3.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "electron": "^9.1.0",
    "eslint": "^7.5.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-loader": "^1.0.0",
    "pino": "^6.3.2",
    "pino-pretty": "^4.0.0",
    "postcss-cli": "^7.1.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "productName": "Browserosaurus"
}
