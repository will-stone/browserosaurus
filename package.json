{
  "name": "browserosaurus",
  "version": "19.0.0",
  "description": "The browser prompter for macOS",
  "keywords": [
    "macOS",
    "Electron",
    "Browser chooser"
  ],
  "homepage": "https://wstone.io/browserosaurus",
  "bugs": {
    "url": "https://github.com/will-stone/browserosaurus/issues"
  },
  "repository": "https://github.com/will-stone/browserosaurus",
  "license": "GPL-3.0-only",
  "author": "Will Stone",
  "main": ".webpack/main",
  "scripts": {
    "doctor": "npm run lint && npm run typecheck && npm run test",
    "icns": "cd ./src/shared/static/icon && ../../../../scripts/png2icns.sh icon.png",
    "lint": "eslint . --ignore-path .gitignore",
    "list-apps": "ts-node ./scripts/listInstalledApps",
    "make": "concurrently \"npm:make:*\" --kill-others-on-fail",
    "make:arm": "electron-forge make --skip-package --arch=arm64",
    "make:intel": "electron-forge make --skip-package --arch=x64",
    "notarize": "node ./scripts/notarize.js",
    "prepackage": "rimraf out",
    "package": "npm run package:arm && npm run package:intel",
    "package:arm": "NODE_ENV=production electron-forge package --platform=darwin --arch=arm64",
    "package:intel": "NODE_ENV=production electron-forge package --platform=darwin --arch=x64",
    "prepare": "husky install",
    "release": "bump --commit \"chore: release v\" && npm run doctor && npm run package && npm run notarize && npm run make",
    "start": "ELECTRON_DISABLE_SECURITY_WARNINGS=true electron-forge start",
    "test": "jest",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "website-css:build": "npx tailwindcss -i node_modules/tailwindcss/tailwind.css -o ./docs/style.css --content docs/index.html -m",
    "website-css:watch": "npx tailwindcss -i node_modules/tailwindcss/tailwind.css -o ./docs/style.css --content docs/index.html -m --watch"
  },
  "config": {
    "forge": "./forge.config.cjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@reduxjs/toolkit": "^1.8.3",
    "app-exists": "^2.1.1",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "electron-log": "^4.4.8",
    "fast-deep-equal": "^3.1.3",
    "file-icon": "^4.0.0",
    "immer": "^9.0.15",
    "lowdb": "^3.0.0",
    "picocolors": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "tidy-url": "^1.5.5",
    "tings": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@electron-forge/cli": "6.0.0-beta.65",
    "@electron-forge/maker-zip": "6.0.0-beta.65",
    "@electron-forge/plugin-webpack": "6.0.0-beta.65",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@timfish/forge-externals-plugin": "^0.2.0",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "@will-stone/eslint-config-base": "^2.0.0",
    "@will-stone/eslint-config-jest": "^2.1.0",
    "@will-stone/eslint-config-node": "^2.0.0",
    "@will-stone/eslint-config-prettier": "^2.0.1",
    "@will-stone/eslint-config-react": "^2.1.0",
    "@will-stone/eslint-config-typescript": "^2.1.0",
    "@will-stone/prettier-config": "^6.0.2",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "electron": "^20.0.0",
    "electron-notarize": "^1.2.1",
    "eslint": "^8.20.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.1",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.7",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "version-bump-prompt": "^6.1.0",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "productName": "Browserosaurus"
}
