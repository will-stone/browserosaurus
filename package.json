{
  "name": "browserosaurus",
  "productName": "Browserosaurus",
  "version": "5.0.0",
  "engines": {
    "node": ">=10.4.1"
  },
  "description": "The browser prompter for macOS",
  "homepage": "http://wstone.io/browserosaurus/",
  "main": "src/main.ts",
  "scripts": {
    "docs": "http-server ./docs",
    "start": "ENV=DEV electron-forge start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx",
    "package": "electron-forge package --platform=darwin --arch=x64",
    "make": "electron-forge make",
    "icns": "cd ./src/images/icon && ./png2icns.sh icon.png"
  },
  "repository": "https://github.com/will-stone/browserosaurus",
  "bugs": {
    "url": "https://github.com/will-stone/browserosaurus/issues"
  },
  "keywords": [
    "Electron",
    "Browser chooser"
  ],
  "author": "Will Stone",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/electron-store": "^1.3.0",
    "@types/jest": "^23.3.13",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.120",
    "@types/mousetrap": "^1.6.1",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.11",
    "@types/xml2js": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-jest": "^23.0.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "http-server": "^0.11.1",
    "jest": "^24.0.0",
    "jest-dom": "^3.0.1",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.0",
    "typescript": "^3.2.0"
  },
  "dependencies": {
    "@artossystems/a": "^3.1.0",
    "electron-compile": "^6.4.4",
    "electron-store": "^2.0.0",
    "immer": "^2.1.3",
    "jsonpath": "^0.2.11",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-transition-group": "^2.4.0",
    "semver": "^5.4.1",
    "styled-components": "^4.2.0",
    "tslib": "^1.4.0",
    "xml2js": "^0.4.19"
  },
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "src/images/icon/icon.icns",
        "ignore": [
          "docs",
          "src/picker/style.scss",
          "__mocks__",
          "node_modules",
          "StartBrowserosaurus.app",
          "StartBrowserosaurus.scpt"
        ],
        "protocols": [
          {
            "name": "HTTP link",
            "schemes": [
              "http",
              "https"
            ]
          },
          {
            "name": "File",
            "schemes": [
              "file"
            ]
          }
        ]
      }
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "react-testing-library/cleanup-after-each",
      "jest-dom/extend-expect"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/docs/",
      "<rootDir>/out/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/docs/",
      "<rootDir>/out/"
    ]
  }
}
